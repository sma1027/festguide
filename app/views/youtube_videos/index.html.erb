<h1><%= link_to @artist.name, artist_path(@artist) %> - youtube index</h1>
<h3>Youtube Name: <%= @artist.youtube_username %></h3>
<h3>Youtube ID: <%= @artist.youtube_playlist_upload_id %></h3>

<ul><% @youtube_videos.each do |video| %>
<li><p><%= link_to image_tag(video.thumbnail), "https://www.youtube.com/watch?v=#{video.video_id}" %></p>
  <p><%= video.title%></p>
  </li>
<% end %>
</ul>